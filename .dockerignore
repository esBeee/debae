# See https://docs.docker.com/engine/reference/builder/#/dockerignore-file for
# more about ignoring files.

# Ignore production configuration for Docker Compose.
docker-compose.production.yml

# Ignore bundler config.
.bundle

# Ignore all logfiles and tempfiles.
log/*
tmp/*
!log/.keep
!tmp/.keep

# Ignore Byebug command history file.
.byebug_history

# Ignore node modules (they will be downloaded before precompiling assets
# automatically).
node_modules
